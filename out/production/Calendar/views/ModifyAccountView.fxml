<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="459.0" prefWidth="642.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ModifyAccountViewController">
   <children>
      <Label layoutX="25.0" layoutY="36.0" text="Modify Account Information:">
         <font>
            <Font name="Arial" size="15.0" />
         </font>
      </Label>
      <VBox layoutX="8.0" layoutY="29.0" prefHeight="373.0" prefWidth="219.0" spacing="15.0" AnchorPane.bottomAnchor="18.0">
         <children>
            <TextField fx:id="firstnameField" />
            <TextField fx:id="lastnameField" />
            <TextField fx:id="emailField" />
            <TextField fx:id="usernameField" />
            <TextField fx:id="genderField" />
            <DatePicker fx:id="dobField" prefHeight="27.0" prefWidth="219.0" />
            <Button fx:id="makeChangeBtn" mnemonicParsing="false" prefHeight="27.0" prefWidth="218.0" text="Make Changes" />
         </children>
      </VBox>
      <MenuBar layoutY="2.0" prefHeight="29.0" prefWidth="642.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <VBox layoutX="300.0" layoutY="147.0" prefHeight="106.0" prefWidth="235.0" spacing="50.0">
         <children>
            <Button fx:id="mainMenuBtn" mnemonicParsing="false" prefHeight="27.0" prefWidth="235.0" text="Main Menu" />
            <Button fx:id="calendarViewBtn" mnemonicParsing="false" prefHeight="27.0" prefWidth="235.0" text="Calendar View" />
            <Button mnemonicParsing="false" onAction="#changePasswordBtnPressed" prefHeight="27.0" prefWidth="237.0" text="Change Password" />
         </children>
      </VBox>
      <Label fx:id="labelField" layoutX="305.0" layoutY="350.0" prefHeight="17.0" prefWidth="235.0" />
   </children>
</AnchorPane>
